<h1> Cats </h1>
  <table>
     <tr>
        <td>Name: </td>
     </tr>
     
     
      <tr>
      <% @cat.each do |cat| %>
        <td><%= cat.name %></td>
     </tr>
     <% end %>
  </table>

<h1> Users </h1>
  <table>
     <tr>
        <td>Username: </td>
        <td>Email: </td>
        <td>Age: </td>
     </tr>
      <tr>
      <% @user.each do |user| %>
        <td><%= user.name %></td>
        <td><%= user.email %></td>
        <td><%= user.age %></td>
     </tr>
     <% end %>
  </table>

<h1> Todos </h1>
  <table>
     <tr>
        <td>Tasks: </td>
        <td>Finished: </td>
     </tr>
     
     
      <tr>
      <% @todo.each do |todo| %>
        <td><%= todo.tasks %></td>
        <td><%= todo.finished %></td>
     </tr>
     <% end %>
  </table>
